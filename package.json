{
  "name": "codelens-golang-navigate",
  "displayName": "codelens-golang-navigate",
  "description": "A Visual Studio Code extension which adds CodeLens actions and hover links for navigating through Golang code.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.107.0"
  },
  "publisher": "hu553in",
  "repository": {
    "type": "git",
    "url": "https://github.com/hu553in/codelens-golang-navigate"
  },
  "license": "MIT",
  "author": {
    "name": "Ruslan Khasanshin",
    "email": "r.m.khasanshin@gmail.com",
    "url": "https://github.com/hu553in"
  },
  "bugs": {
    "url": "https://github.com/hu553in/codelens-golang-navigate/issues",
    "email": "r.m.khasanshin@gmail.com"
  },
  "homepage": "https://github.com/hu553in/codelens-golang-navigate",
  "keywords": [
    "golang",
    "vscode",
    "cursor",
    "codelens",
    "hover",
    "navigation"
  ],
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "extensionDependencies": [
    "golang.go"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "codelensGolangNavigate",
      "properties": {
        "codelensGolangNavigate.enableCodeLensActions": {
          "type": "boolean",
          "default": true
        },
        "codelensGolangNavigate.enableHoverLinks": {
          "type": "boolean",
          "default": true
        }
      }
    },
    "commands": [
      {
        "command": "codelensGolangNavigate.goToDefinitionAt",
        "title": "Go to definition"
      },
      {
        "command": "codelensGolangNavigate.goToTypeDefinitionAt",
        "title": "Go to type definition"
      },
      {
        "command": "codelensGolangNavigate.goToImplementationAt",
        "title": "Go to implementations"
      },
      {
        "command": "codelensGolangNavigate.goToReferencesAt",
        "title": "Go to references"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint --fix && prettier --write .",
    "test": "vscode-test",
    "release:patch": "pnpm version patch -m \"chore(release): %s\"",
    "release:minor": "pnpm version minor -m \"chore(release): %s\"",
    "release:major": "pnpm version major -m \"chore(release): %s\"",
    "preversion": "pnpm run test",
    "postversion": "git push origin main --follow-tags",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,js,json,yaml,yml}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "25.x",
    "@types/vscode": "^1.107.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  }
}
