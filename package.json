{
  "name": "codelens-golang-navigate",
  "displayName": "codelens-golang-navigate",
  "description": "A Visual Studio Code extension which adds CodeLens actions and hover links for navigating through Golang code.",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.90.0"
  },
  "icon": "assets/icon.png",
  "publisher": "hu553in",
  "repository": {
    "type": "git",
    "url": "https://github.com/hu553in/codelens-golang-navigate"
  },
  "license": "MIT",
  "author": {
    "name": "Ruslan Khasanshin",
    "email": "r.m.khasanshin@gmail.com",
    "url": "https://github.com/hu553in"
  },
  "bugs": {
    "url": "https://github.com/hu553in/codelens-golang-navigate/issues",
    "email": "r.m.khasanshin@gmail.com"
  },
  "homepage": "https://github.com/hu553in/codelens-golang-navigate",
  "keywords": [
    "golang",
    "vscode",
    "cursor",
    "codelens",
    "hover",
    "navigation"
  ],
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "extensionDependencies": [
    "golang.go"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "codelensGolangNavigate",
      "properties": {
        "codelensGolangNavigate.enableCodeLensActions": {
          "type": "boolean",
          "default": true,
          "description": "Show CodeLens actions above Go symbols."
        },
        "codelensGolangNavigate.enableHoverLinks": {
          "type": "boolean",
          "default": true,
          "description": "Show trusted hover links when hovering a Go symbol."
        },
        "codelensGolangNavigate.logLevel": {
          "type": "string",
          "default": "info",
          "enum": [
            "error",
            "warn",
            "info",
            "debug"
          ],
          "description": "Minimum log level written to the extension Output Channel."
        },
        "codelensGolangNavigate.refreshOnTyping": {
          "type": "boolean",
          "default": true,
          "description": "Refresh CodeLens when the active Go file changes (typing/paste)."
        },
        "codelensGolangNavigate.refreshDebounceMs": {
          "type": "number",
          "default": 120,
          "minimum": 0,
          "description": "Debounce delay (ms) for refresh triggers."
        }
      }
    },
    "commands": [
      {
        "command": "codelensGolangNavigate.definition",
        "title": "codelens-golang-navigate: Definition"
      },
      {
        "command": "codelensGolangNavigate.typeDefinition",
        "title": "codelens-golang-navigate: Type definition"
      },
      {
        "command": "codelensGolangNavigate.implementations",
        "title": "codelens-golang-navigate: Implementations"
      },
      {
        "command": "codelensGolangNavigate.typeHierarchy",
        "title": "codelens-golang-navigate: Type hierarchy"
      },
      {
        "command": "codelensGolangNavigate.callers",
        "title": "codelens-golang-navigate: Callers"
      },
      {
        "command": "codelensGolangNavigate.references",
        "title": "codelens-golang-navigate: References"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "prepare": "husky",
    "pretest": "pnpm run compile",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "test": "vscode-test --coverage --coverage-reporter json-summary --coverage-reporter text --coverage-reporter html",
    "coverage": "./scripts/verify-test-coverage.sh",
    "check": "pnpm run lint && pnpm run test && pnpm run coverage",
    "check:fix": "pnpm run lint:fix && pnpm run test && pnpm run coverage",
    "release:patch": "pnpm version patch -m \"chore(release): %s\"",
    "release:minor": "pnpm version minor -m \"chore(release): %s\"",
    "release:major": "pnpm version major -m \"chore(release): %s\"",
    "preversion": "./scripts/verify-clean-main-branch.sh && pnpm run test && pnpm run coverage",
    "postversion": "git push origin main --follow-tags"
  },
  "lint-staged": {
    "*.{ts,js,json,yaml,yml}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "25.x",
    "@types/vscode": "^1.90.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  }
}
